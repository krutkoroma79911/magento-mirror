<?xml version="1.0"?>
<config>
    <modules>
        <Tsg_Trial>
            <version>0.0.1</version>
        </Tsg_Trial>
    </modules>
    <global>
        <blocks>
            <tsg_trial>
                <class>Tsg_Trial_Block</class>
            </tsg_trial>
        </blocks>
    </global>
    <frontend>
        <routers>
            <Tsg_Trial_Router>
                <use>standard</use>
                <args>
                    <module>Tsg_Trial</module>
                    <frontName>trial</frontName>
                </args>
            </Tsg_Trial_Router>
        </routers>
        <layout>
            <updates>
                <tsg_trial>
                    <file>trial.xml</file>
                </tsg_trial>
            </updates>
        </layout>
    </frontend>
</config>